type Query {
    artistById(id: ID): Artist!
    artists: [Artist]
}

type Mutation {
    createArtist(artistInput: ArtistInput): Artist
}

input ArtistInput {
    name: String!
}

type Artist {
    id: ID!
    name: String!
    albums: [Album]
}

type Album {
    title: String!
    date: String!
    category: String!
}

